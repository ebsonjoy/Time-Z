function cancelCoupon() {
    fetch('/cancel-coupon', {
        method: 'POST'
    })
    .then(response => {
        if (!response.ok) {
            throw new Error('Failed to cancel coupon');
        }
        return response.json();
    })
    .then(data => {
        console.log('Coupon canceled successfully');
        const originalTotalPrice = data.originalTotalPrice.toFixed(2);
        document.getElementById('subtotal').innerText =  originalTotalPrice; // Update the displayed total price
        document.getElementById('totalPriceInput').value = originalTotalPrice;
    })
    .catch(error => {
        console.error('Error canceling coupon:', error);
    });
}
