      function validateAddress(input) {
          var value = input.value.trim();
          var errorField = input.nextElementSibling;
          if (value === "") {
              errorField.innerHTML = "<span style='color: red;'>Address is required</span>";
          } else {
              errorField.innerHTML = "<span style='color: green;'>Valid</span>";
          }
          validateForm();
      }
  
