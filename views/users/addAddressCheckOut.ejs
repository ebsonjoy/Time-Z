      function validateState(input) {
          var value = input.value.trim();
          var errorField = input.nextElementSibling;
          if (value === "") {
              errorField.innerHTML = "<span style='color: red;'>State is required</span>";
          } else {
              errorField.innerHTML = "<span style='color: green;'>Valid</span>";
          }
          validateForm();
      }
  
      function validateZIP(input) {
          var value = input.value.trim();
          var errorField = input.nextElementSibling;
          var zipPattern = /^\d{6}$|^\d{6}-\d{5}$/; // US ZIP code pattern
          if (value === "") {
              errorField.innerHTML = "<span style='color: red;'>ZIP Code is required</span>";
          } else if (!zipPattern.test(value)) {
              errorField.innerHTML = "<span style='color: red;'>Invalid ZIP Code</span>";
          } else {
              errorField.innerHTML = "<span style='color: green;'>Valid</span>";
          }
          validateForm();
      }
  
      function validateCountry(input) {
          var value = input.value.trim();
          var errorField = input.nextElementSibling;
          if (value === "") {
              errorField.innerHTML = "<span style='color: red;'>Country is required</span>";
          } else {
              errorField.innerHTML = "<span style='color: green;'>Valid</span>";
          }
          validateForm();
      }
  
      function validateForm() {
          var submitBtn = document.getElementById("submit-btn");
          var inputs = document.querySelectorAll("input[required]");
          var isValid = true;
          inputs.forEach(function(input) {
              var value = input.value.trim();
              var errorField = input.nextElementSibling;
              if (value === "") {
                  isValid = false;
              } else if (errorField.textContent.includes("Invalid")) {
                  isValid = false;
              }
          });
          submitBtn.disabled = !isValid;
      }
  </script>
