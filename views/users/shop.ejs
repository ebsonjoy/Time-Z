            <a href="index.html" rel="nofollow">Home</a>
            <span></span> Shop
          </div>
        </div>
      </div>
      <section class="mt-50 mb-50">
        <div class="container">
            <div class="row">
                <div class="col-lg-9">
                    <div class="row product-grid-3">
                        <% prod.forEach(row => { %>
                        <div class="col-lg-4 col-md-4 col-12 col-sm-6">
                            <div class="product-cart-wrap mb-30">
                                <div class="product-img-action-wrap">
                                    <div class="product-img product-img-zoom">
                                        <a href="/viewDetails/<%= row._id%>">
                                            <img class="default-img" src="/static/products/<%= row.images[0] %>" alt="">
                                            <img class="hover-img" src="/static/products/<%= row.images[1] %>" alt="">
                                        </a>
                                    </div>
                                    <div class="product-badges product-badges-position product-badges-mrg">
                                        <span class="hot">Hot</span>
                                    </div>
                                </div>
                                <div class="product-content-wrap">
                                    <div class="product-category">
                                        <a href="#"><%= row.brand %></a>
                                    </div>
                                    <h2 style="overflow: hidden;display: -webkit-box;-webkit-line-clamp: 1;-webkit-box-orient: vertical;">
                                        <a href="/viewDetails/<%= row._id%>"><%= row.product %></a>
                                    </h2>
                                    <div class="rating-result" title="90%">
                                        <span><span>90%</span></span>
                                    </div>
                                    <div class="product-price">
                                        <span>₹<%= row.price %></span>
                                        <span class="old-price">₹<%= row.oldprice %></span>
                                    </div>
                                    <h2><a href="/viewDetails/<%= row._id%>">View Details</a></h2>
                                    <div class="product-action-1 show">
                                        <button aria-label="Add To Cart" class="action-btn hover-up" onclick="cart('<%=row.id %>')">
                                            <i class="fi-rs-shopping-bag-add"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <% }) %>
                    </div>
                </div>
                <div class="col-lg-3 primary-sidebar sticky-sidebar">
                    <div class="widget-category mb-30">
                        <h5 class="section-title style-1 mb-30 wow fadeIn animated">Category</h5>
                        <ul class="categories">
                            <% cate.forEach(row => { %>
                            <li>
                                <a href="/categoryFilter/<%= row._id %>"><%= row.category %></a>
                            </li>
                            <% }) %>
                        </ul>
                    </div>
                   
                   <!-- Fillter By Price -->
                   <div class="widget-category mb-30">
                    <h5 class="section-title style-1 mb-30 wow fadeIn animated">Price Range</h5>
                    <ul class="categories">
                   <!-- Price Range Widget Markup -->
                   <li>
                  <a href="/priceFilter/<%= text %>?sort=lowToHigh">Sort by Price (Low to High)</a><br>
                </li>
                  <li>
                   <a href="/priceFilter/<%= text %>?sort=highToLow">Sort by Price (High to Low)</a>
                  </li>
                  </ul>
                   </div>

                    <!-- Other Sidebar Widgets -->
                </div> 
            </div>
            <!-- Pagination Markup -->
            <div class="pagination-area mt-15 mb-sm-5 mb-lg-0">
              <nav aria-label="Page navigation example">
                  <ul class="pagination justify-content-start">
                      <% if (currentPage > 1) { %>
                          <li class="page-item"><a class="page-link" href="/shop/<%= text ? text + '/' : '' %><%= currentPage - 1 %>"><i class="fi-rs-angle-double-small-left"></i></a></li>
                      <% } %>
                      <% for (let i = 1; i <= totalPages; i++) { %>
                          <li class="page-item <%= currentPage == i ? 'active' : '' %>"><a class="page-link" href="/shop/<%= text ? text + '/' : '' %><%= i %>"><%= i %></a></li>
                      <% } %>
                      <% if (currentPage < totalPages) { %>
                          <li class="page-item"><a class="page-link" href="/shop/<%= text ? text + '/' : '' %><%= currentPage + 1 %>"><i class="fi-rs-angle-double-small-right"></i></a></li>
                      <% } %>
                  </ul>
              </nav>
          </div>
          

        </div>
    </section>
